pipeline{
    agent any
    stages{
        stage('scm checkout'){
            steps{
                git branch: 'master', url: 'https://github.com/Abhinav501-eng/maven-project18.git'
            }
        }

        stage('code compile'){
            steps{
                withMaven(globalMavenSettingsConfig: '', jdk: 'JAVA_HOME', maven: 'MAVEN_HOME', mavenSettingsConfig: '', traceability: true) {
               sh 'mvn compile'
              }
            }
        }

        stage('code test'){
            steps{
                withMaven(globalMavenSettingsConfig: '', jdk: 'JAVA_HOME', maven: 'MAVEN_HOME', mavenSettingsConfig: '', traceability: true) {
               sh 'mvn test'
            }
            }
        }
    }
}